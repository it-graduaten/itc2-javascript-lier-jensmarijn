    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\Multiplayer Quiz - opgave\\src\\index.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "name": "Launch browser against localhost",
            "type": "msedge",
            "request": "launch",
            "url": "http://localhost:8080/",
            "webRoot": "${workspaceFolder}"
        },
    ]
}